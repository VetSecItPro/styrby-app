{
  "name": "styrby-mobile",
  "version": "0.1.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "dev": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "prebuild": "node scripts/validate-config.mjs && expo prebuild",
    "build:dev": "npm run validate && eas build --profile development",
    "build:preview": "npm run validate && eas build --profile preview",
    "build:prod": "npm run validate && eas build --profile production",
    "validate": "node scripts/validate-config.mjs",
    "generate-assets": "node scripts/generate-placeholders.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ app/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/native": "^7.0.0",
    "@supabase/supabase-js": "^2.47.10",
    "clsx": "^2.1.1",
    "expo": "~52.0.0",
    "expo-camera": "~16.0.0",
    "expo-clipboard": "~7.0.1",
    "expo-constants": "~17.0.0",
    "expo-device": "~7.0.3",
    "expo-haptics": "^15.0.8",
    "expo-linking": "~7.0.0",
    "expo-notifications": "~0.29.14",
    "expo-router": "~4.0.0",
    "expo-secure-store": "~14.0.0",
    "expo-splash-screen": "~0.29.0",
    "expo-sqlite": "~15.1.4",
    "expo-status-bar": "~2.0.0",
    "nativewind": "^4.0.0",
    "react": "18.3.1",
    "react-native": "0.76.0",
    "react-native-gesture-handler": "~2.20.0",
    "react-native-pager-view": "6.5.1",
    "react-native-reanimated": "~3.16.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.0.0",
    "react-native-svg": "15.8.0",
    "@styrby/shared": "workspace:*",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@eslint/js": "^9.20.0",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~8.0.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.6",
    "react-test-renderer": "^18.3.1",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.6.0",
    "typescript-eslint": "^8.30.0"
  }
}
